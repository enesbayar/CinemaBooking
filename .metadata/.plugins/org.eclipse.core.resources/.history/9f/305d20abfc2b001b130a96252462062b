package Services;

import java.sql.Connection;
import java.sql.ResultSet;
import java.sql.SQLException;
import java.sql.Statement;
import java.util.ArrayList;

import Model.Admin;

public class AdminServices implements IAdminServices {
	SQLConnect sqlConnect = new SQLConnect();
	Connection con = sqlConnect.connDB();
	Statement statement = null;
	ResultSet resultSet = null;
	@Override
	public void login() {
		ArrayList<Admin> adminList = new ArrayList();
		Admin admin;
		try{
			statement = con.createStatement();
			resultSet = statement.executeQuery("SELECT * FROM tblAdmin");
			while(resultSet.next()){
				admin = new Admin(resultSet.getInt("ID"), resultSet.getString("userName"), resultSet.getString("password"));
				
			}
			
		}catch(SQLException e){
			e.printStackTrace();
		}finally{
			
		}
	}
	
}
