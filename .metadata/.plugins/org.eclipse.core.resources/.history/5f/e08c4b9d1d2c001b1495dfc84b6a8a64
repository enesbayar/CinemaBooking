package Model;
import java.util.ArrayList;


public class Movie {

	private String movieName;
	private String movieUrl;
	private String description;
	private ArrayList<String> sessions;
	private double movieRate;
	private double Price;
	public String getMovieName() {
		return movieName;
	}
	public Movie(String movieName, String movieUrl, String description,
			ArrayList<String> session, double movieRate, double price) {
		super();
		this.movieName = movieName;
		this.movieUrl = movieUrl;
		this.description = description;
		this.sessions = session;
		this.movieRate = movieRate;
		Price = price;
	}
	public Movie() {
		super();
	}
	public String getMovieUrl() {
		return movieUrl;
	}
	public void setMovieUrl(String movieUrl) {
		this.movieUrl = movieUrl;
	}
	public String getDescription() {
		return description;
	}
	public void setDescription(String description) {
		this.description = description;
	}
	public ArrayList<String> getSessions() {
		return sessions;
	}
	public void setSessions(ArrayList<String> sessions) {
		this.sessions = sessions;
	}
	public double getMovieRate() {
		return movieRate;
	}
	public void setMovieRate(double movieRate) {
		this.movieRate = movieRate;
	}
	public double getPrice() {
		return Price;
	}
	public void setPrice(double price) {
		Price = price;
	}
	public void setMovieName(String movieName) {
		this.movieName = movieName;
	}
	
	
	
	
}
