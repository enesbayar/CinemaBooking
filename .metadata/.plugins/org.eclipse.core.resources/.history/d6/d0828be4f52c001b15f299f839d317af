package Services;

import java.util.ArrayList;

public class deneme {
	
	public static void main(String[] args) {
		String deneme = "D;B;B;D;B;D;B;D;D;B;B;B;B;D;B;D;B;D;D;D;B;B;B";
		System.out.println(deneme);
		String[] arraylist;
		arraylist =  deneme.split(";");
		System.out.println(arraylist);
	}
}
